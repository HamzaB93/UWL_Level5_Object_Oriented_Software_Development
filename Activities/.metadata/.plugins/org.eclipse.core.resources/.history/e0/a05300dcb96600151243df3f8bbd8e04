package week2Aggregation;

import java.util.Date;

public class Member 
{
	////////// Properties //////////
	
	private long id;
	private String name;
	private Date dateCreated;
	// Association, reflexive 
	Member Supervisor;
	
	
	////////// Constructors //////////
	
	// Default constructor
	public Member()
	{
		id = 0000;
		name = "Unknown";
		dateCreated = new Date();
	}
	
	// Parameterised constructor
	public Member(long id, String name, Date dateCreated)
	{
		this.id = id;
		this.name = name;
		this.dateCreated = dateCreated;
	}
	
	
	////////// Accessors for Member //////////
	
	public long getId()
	{
		return id;
	}
	public String getName()
	{
		return name;
	}
	public Date getDateCreated()
	{
		return dateCreated;
	}
	
	
}
